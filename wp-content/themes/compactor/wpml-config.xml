<wpml-config>
  <shortcodes>
    <shortcode>
      <tag>compactor_button</tag>
      <attributes>
        <attribute>compactor_btn_text</attribute>
        <attribute type="link" encoding="vc_link ">compactor_btn_link</attribute>

        <attribute>compactor_btn_style</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>webdevia_testimonials</tag>
      <attributes>
        <attribute>testimonial_items_to_show</attribute>
        <attribute>testimonial_item_todisplay</attribute>
        <attribute>testimonial_layout</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>wd_pricing_table</tag>
      <attributes>
        <attribute>style</attribute>
        <attribute>featured</attribute>
        <attribute type="link" encoding="vc_link ">button_link</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>wd_headings</tag>
      <attributes>
        <attribute>title</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>wd_blog</tag>
      <attributes>
        <attribute>itemperpage</attribute>
        <attribute>columns</attribute>
        <attribute>show_thumbnail</attribute>
        <attribute>show_meta</attribute>
      </attributes>
    </shortcode>
    <shortcode>
      <tag>compactor_percentage_circle</tag>
      <attributes>
        <attribute>compactor_circle_size</attribute>
        <attribute>compactor_circle_img</attribute>
      </attributes>
    </shortcode>
  </shortcodes>
  <custom-fields>
    <custom-field action="translate">title</custom-field>
    <custom-field action="translate">description</custom-field>
    <custom-field action="translate">keywords</custom-field>
  </custom-fields>
  <custom-types>
    <custom-type translate="1">portfolio</custom-type>
    <custom-type translate="1">team members</custom-type>
    <custom-type translate="1">testimonails</custom-type>
  </custom-types>
  <taxonomies>
    <taxonomy translate="1">genre</taxonomy>
    <taxonomy translate="1">type</taxonomy>
    <taxonomy translate="0">publisher</taxonomy>
  </taxonomies>
  <admin-texts>
    <key name="menu_style"/>
    <key name="menu_mobile_layout"/>
    <key name="header_contain_to_grid"/>
    <key name="action_button"/>
    <key name="wd_404_page"/>
    <key name="show_the_logo"/>
    <key name="logo_link"/>
    <key name="compactor_favicon_icon_path"/>
    <key name="compactor_menu_style"/>
    <key name="compactor_bg_single_post_path"/>
    <key name="compactor_primary_color"/>
    <key name="compactor_primary_color"/>
    <key name="compactor_secondary_color"/>
    <key name="compactor_footer_columns"/>
    <key name="compactor_copyright"/>

  </admin-texts>

  <language-switcher-settings>
    <key name="icl_lang_sel_config">
      <key name="font-current-normal">#444444</key>
      <key name="font-current-hover">#000000</key>
      <key name="background-current-normal">#ffffff</key>
      <key name="background-current-hover">#eeeeee</key>
      <key name="font-other-normal">#444444</key>
      <key name="font-other-hover">#000000</key>
      <key name="background-other-normal">#ffffff</key>
      <key name="background-other-hover">#eeeeee</key>
      <key name="border">#cdcdcd</key>
    </key>
    <key name="icl_lang_sel_footer_config">
      <key name="font-current-normal">#444444</key>
      <key name="font-current-hover">#000000</key>
      <key name="background-current-normal">#ffffff</key>
      <key name="background-current-hover">#eeeeee</key>
      <key name="font-other-normal">#444444</key>
      <key name="font-other-hover">#000000</key>
      <key name="background-other-normal">#ffffff</key>
      <key name="background-other-hover">#eeeeee</key>
      <key name="border">#cdcdcd</key>
    </key>
    <key name="icl_language_switcher_sidebar">0</key>
    <key name="icl_widget_title_show">0</key>
    <key name="icl_lang_sel_type">dropdown</key>
    <key name="icl_lso_link_empty">0</key>
    <key name="icl_lso_flags">0</key>
    <key name="icl_lso_native_lang">1</key>
    <key name="icl_lso_display_lang">1</key>
    <key name="icl_lang_sel_footer">0</key>
    <key name="icl_post_availability">0</key>
    <key name="icl_post_availability_position">below</key>
    <key name="icl_post_availability_text">This post is also available in: %s</key>
  </language-switcher-settings>
</wpml-config>